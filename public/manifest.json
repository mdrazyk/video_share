{
	"name": "Video Share",
	"description": "Chrome plugin for downloading videos from closed Facebook groups",
	"version": "0.1",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": "favicon.ico",
		"default_popup": "index.html"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"content-script.js"
			]
		}
	]
}
